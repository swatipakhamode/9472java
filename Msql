use students_db;
select first_name   from worker_tbl;
select upper(first_name) from worker_tbl;
select distinct worker_department from worker_tbl ;
select substring(first_name,1,3) from worker_tbl;
select instr(first_name,Binary's') from worker_tbl where first_name='swati';
select rtrim(first_name) from worker_tbl;
select ltrim(worker_department) from worker_tbl;
select distinct length(worker_department) from worker_tbl;
select replace(first_name,'m','M') from worker_tbl ;
select concat(first_name,' ',last_name) as COMPLETE_NAME from worker_tbl;
select * from worker_tbl order by first_name asc;
select * from worker_tbl order by first_name asc , worker_department desc;
select * from worker_tbl where first_name in ('swati','renu');
select * from worker_tbl where first_name not in ('swati','renu');
select * from worker_tbl where worker_department like 'admin%';
select * from worker_tbl where first_name like '%s%';
select * from worker_tbl where first_name like '%m';
select * from worker_tbl where first_name like '______m';
select * from worker_tbl where worker_salary between 100000 and 500000;
select sum(worker_department='admin') from worker_tbl; /*(OR)*/ select count(*) from worker_tbl where worker_department='admin';





















